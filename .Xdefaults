 ! General
 URxvt.transparent: true
 urxvt*shading: 30
 !urxvt*termName:  rxvt-256color
 urxvt*loginShell:         true
 urxvt*scrollBar:         false
 urxvt*secondaryScroll:    true
 urxvt*saveLines:         65535
 urxvt*cursorBlink:       false
 urxvt*urgentOnBell:       true
 !urxvt*override-redirect:false
 !urxvt*borderLess:       true
 !urxvt*internalBorder:       50
 !urxvt*externalBorder:       50
 URxvt.intensityStyles: false

 ! Extensions
 urxvt*perl-lib:        /usr/lib/urxvt/perl/
 urxvt*perl-ext-common: default,matcher,searchable-scrollback,color-themes
 urxvt*urlLauncher:     /usr/bin/firefox
 urxvt*matcher.button:  1
 !  - catch ugly URLs
 urxvt.cutchars:        "()'*<>[]{|}"

 ! Appearance
 !  - use a bitmap font
urxvt.font:             xft:Bitstream Vera Sans Mono:pixelsize=13
 urxvt.boldFont:         xft:Bitstream Vera Sans Mono:bold:pixelsize=13
 ! URxvt*font:        -*-terminus-medium-*-*-*-10-*-*-*-*-*-*-iso8859-1
 !urxvt*boldFont:    -*-terminus-bold-*-*-*-10-*-*-*-*-*-*-iso8859-1
 !urxvt*font:        -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
 !urxvt*boldFont:    -xos4-terminus-bold-*-*-*-12-*-*-*-*-*-*-*
 !  - use xft for drawing fonts
 urxvt*font:       xft:Terminus:14


